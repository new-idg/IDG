#import <UIKit/UIKit.h>
#import "ChooseFlavorModel.h"

@interface StallCell : UITableViewCell
/** 菜图片 */
@property (nonatomic, strong) UIImageView *headerImage;
/** 英文名 */
@property (nonatomic, strong) UILabel *englishName;
/** 菜名 */
@property (nonatomic, strong) UILabel *vegetable;
/** 菜品介绍 */
@property (nonatomic, strong) UILabel *vegetableIntroduce;
/** 销售额 */
@property (nonatomic, strong) UILabel *salesVolume;
/** 卡路里 */
@property (nonatomic, strong) UILabel *calorie;
/** 价格 */
@property (nonatomic, strong) UILabel *price;
/** +号按钮 */
@property (nonatomic, strong) UIButton *addBtn;
/** 数量label */
@property (nonatomic, strong) UILabel *numberLabel;
/** -号按钮 */
@property (nonatomic, strong) UIButton *minusBtn;

-(instancetype)initWithStyle:(UITableViewCellStyle)style reuseIdentifier:(NSString *)reuseIdentifier;
-(void)setCellModel:(ChooseFlavorModel *)model;
@end
